{
  "name": "laserweb-builder",
  "version": "1.0.0",
  "description": "Builds LaserWeb",
  "main": "node_modules/lw.comm-server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "newcomms": "yarn upgrade lw.comm-server --latest",
    "rebuild": "./node_modules/.bin/electron-rebuild"
  },
  "postinstall": "electron-rebuild",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LaserWeb/LaserWeb-binaries.git"
  },
  "author": "The LaserWeb Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LaserWeb/LaserWeb-binaries/issues"
  },
  "homepage": "https://github.com/LaserWeb/LaserWeb-binaries#readme",
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron/rebuild": "^3.6.0",
    "copyfiles": "^1.2.0",
    "electron": "^26.6.10",
    "ncp": "^2.0.x",
    "node-gyp": "^10.1.0"
  },
  "dependencies": {
    "electron-download-tf": "^4.3.4",
    "electron-squirrel-startup": "^1.0.1",
    "lw.comm-server": "file:../lw.comm-server",
    "ip": "^1.1.5",
    "node-abi": "^3.45.0",
    "node-hid": "^2.2.0",
    "node-static": "*",
    "request": "*",
    "serialport": "9.x",
    "socket.io": "4.x",
    "ws": "8.x",
    "yarn": "^1.22.19"
  },
  "build": {
    "appId": "xyz.laserweb",
    "productName": "LaserWeb",
    "asar": true,
    "files": [
      "**/*"
    ],
    "mac": {
      "icon": "build/icon.icns",
      "category": "public.app-category.graphics-design"
    },
    "dmg": {
      "icon": "build/icon.icns",
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "linux": {
      "category": "Graphics"
    }
  },
  "keywords": [
    "laserweb",
    "cncweb",
    "grbl",
    "smoothieware",
    "tinyg",
    "usb",
    "serial",
    "ethernet",
    "gateway",
    "ESP8266"
  ]
}
