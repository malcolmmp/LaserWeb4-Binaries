{
  "name": "laserweb-builder",
  "version": "1.0.0",
  "description": "Builds laserweb",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "start": "./node_modules/.bin/electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rottschaferanders/LaserWeb-binaries.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Rottschaferanders/LaserWeb-binaries/issues"
  },
  "homepage": "https://github.com/Rottschaferanders/LaserWeb-binaries#readme",
  "dependencies": {
    "electron-updater": "^2.21.10",
    "lw.comm-server": "github:Rottschaferanders/lw.comm-server",
    "ncp": "^2.0.0",
    "yarn": "^1.22.19",
    "serialport": "^9.2.8"
  },
  "devDependencies": {
    "copyfiles": "^1.2.0",
    "electron": "^1.6.x",
    "electron-builder": "^14.1.1",
    "electron-rebuild": "^2.3.5",
    "ncp": "^2.0.0"
  },
  "build": {
    "appId": "xyz.laserweb",
    "productName": "LaserWeb",
    "asar": true,
    "files": [
      "**/*"
    ],
    "mac": {
      "icon": "build/icon.icns",
      "category": "public.app-category.graphics-design"
    },
    "dmg": {
      "icon": "build/icon.icns",
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "linux": {
      "category": "Graphics"
    }
  },
  "keywords": [
    "laserweb",
    "cncweb",
    "grbl",
    "smoothieware",
    "tinyg",
    "usb",
    "serial",
    "ethernet",
    "gateway",
    "ESP8266"
  ]
}
